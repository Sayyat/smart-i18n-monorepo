{
  "languages": [
    "kk",
    "ru",
    "en"
  ],
  "fallbackLanguage": "en",
  "defaultNS": "translation",
  "configFilePath": "src/i18n/lib/config.ts",
  "localesDirectory": "src/i18n/locales",
  "generatedNamespacesPath": "src/i18n/generated/namespaces.ts",
  "generatedTypesPath": "src/i18n/generated/types.ts",
  "keepUnusedKeys": true,
  "includePatterns": [
    "src/**/*.{js,ts,jsx,tsx}"
  ],
  "excludePatterns": [
    "**/node_modules/**",
    "src/i18n/**"
  ]
}